<?xml version="1.0" encoding="utf-8"?>
<com.app.roadz.modules.Home.Menu.MenuListRow_ xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:clickable="true"
    android:foreground="?attr/selectableItemBackground"
    android:layoutDirection="locale"
    android:orientation="vertical"
    android:paddingTop="@dimen/d8"
    android:paddingBottom="@dimen/d8">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/menu_icon"
            android:layout_width="@dimen/d18"
            android:layout_height="@dimen/d24"
            android:layout_marginStart="@dimen/d10"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_menu_car" />
        <!--android:tint="#333132"-->

        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start|center"
            android:layout_marginStart="@dimen/d14"
            android:ellipsize="end"
            android:fontFamily="@font/hkgrotesk_bold"
            android:gravity="start|center"
            android:maxLines="2"
            android:text="Home"
            android:textColor="#333132"
            android:textSize="@dimen/sp16" />
    </LinearLayout>


</com.app.roadz.modules.Home.Menu.MenuListRow_>