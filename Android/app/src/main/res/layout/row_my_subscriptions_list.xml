<?xml version="1.0" encoding="utf-8"?>
<com.app.roadz.modules.MySubscriptions.MySubscriptionsListRow_ xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layoutDirection="locale"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="start|center"
        android:orientation="vertical"
        android:paddingLeft="@dimen/d20"
        android:paddingRight="@dimen/d20">

        <LinearLayout
            android:id="@+id/titleView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:foreground="?android:attr/selectableItemBackground"
            android:paddingTop="@dimen/d6"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/d6">

            <TextView
                android:id="@+id/tv_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/hkgrotesk_bold"
                android:gravity="start|center"
                android:paddingTop="@dimen/d10"
                android:paddingBottom="@dimen/d10"
                android:text="Emergecy"
                android:textAlignment="viewStart"
                android:textColor="#263231"
                android:textSize="@dimen/sp17" />
            <TextView
                android:id="@+id/tv_status"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight=".3"
                android:fontFamily="@font/hkgrotesk_bold"
                android:gravity="start|center_vertical"
                android:paddingTop="@dimen/d10"
                android:paddingBottom="@dimen/d10"
                android:text=""
                android:textAlignment="viewStart"
                android:textColor="#263231"
                android:textSize="@dimen/sp10" />
            <ImageView
                android:id="@+id/indicatorIcon"
                android:layout_width="@dimen/d14"
                android:layout_height="@dimen/d14"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/d10"
                android:layout_marginEnd="@dimen/d10"
                android:rotation="180"
                android:src="@drawable/ic_spinner_indicator"
                android:tint="#848384" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height=".7dp"
            android:background="#848384" />


        <net.cachapa.expandablelayout.ExpandableLayout
            android:id="@+id/expandable_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/d5"
            android:paddingTop="@dimen/d10"
            android:paddingBottom="@dimen/d10"
            app:el_duration="500"
            app:el_expanded="true"
            app:el_parallax="0.5">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d5"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="@string/order_id"
                        android:textAlignment="viewStart"
                        android:textColor="#333132"
                        android:textSize="@dimen/sp16" />

                    <TextView
                        android:id="@+id/tv_OrderID"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_weight="1.2"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="LT123456"
                        android:textAlignment="viewStart"
                        android:textColor="#848384"
                        android:textSize="@dimen/sp16" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d5"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="@string/car_model_"
                        android:textAlignment="viewStart"
                        android:textColor="#333132"
                        android:textSize="@dimen/sp16" />

                    <TextView
                        android:id="@+id/tv_CarModel"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_weight="1.2"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="Toyota Camry 2018"
                        android:textAlignment="viewStart"
                        android:textColor="#848384"
                        android:textSize="@dimen/sp16" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d5"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="@string/start_date"
                        android:textAlignment="viewStart"
                        android:textColor="#333132"
                        android:textSize="@dimen/sp16" />

                    <TextView
                        android:id="@+id/tv_StartDate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_weight="1.2"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="20 / 01 / 2018"
                        android:textAlignment="viewStart"
                        android:textColor="#848384"
                        android:textSize="@dimen/sp16" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d5"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="@string/end_date"
                        android:textAlignment="viewStart"
                        android:textColor="#333132"
                        android:textSize="@dimen/sp16" />

                    <TextView
                        android:id="@+id/tv_EndDate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_weight="1.2"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="22 / 01 / 2019"
                        android:textAlignment="viewStart"
                        android:textColor="#848384"
                        android:textSize="@dimen/sp16" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d5"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="@string/amount"
                        android:textAlignment="viewStart"
                        android:textColor="#333132"
                        android:textSize="@dimen/sp16" />

                    <TextView
                        android:id="@+id/tv_Amount"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_weight="1.2"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="@string/kd_10_000"
                        android:textAlignment="viewStart"
                        android:textColor="#848384"
                        android:textSize="@dimen/sp16" />
                </LinearLayout>
                <LinearLayout
                    android:gravity="end"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d5"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/btn_pay"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_20sdp"
                        android:background="@drawable/btn_bg"
                        android:text="@string/pay"
                        android:textAlignment="center"
                        android:layout_gravity="center"
                        android:textColor="#fff"
                        android:textSize="@dimen/sp16" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d5"
                    android:visibility="gone"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="@string/status"
                        android:textAlignment="viewStart"
                        android:textColor="#333132"
                        android:textSize="@dimen/sp16" />

                    <TextView
                        android:id="@+id/tv_Status"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_weight="1.2"
                        android:fontFamily="@font/hkgrotesk_bold"
                        android:gravity="start|center"
                        android:text="Pending"
                        android:textAlignment="viewStart"
                        android:textColor="#848384"
                        android:textSize="@dimen/sp16" />

                    <TextView
                        android:id="@+id/signupBtn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="@drawable/btn_bg"
                        android:fontFamily="@font/hkgrotesk_medium"
                        android:foreground="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:lines="1"
                        android:minHeight="@dimen/d34"
                        android:paddingLeft="@dimen/d20"
                        android:paddingRight="@dimen/d20"
                        android:text="@string/renew"
                        android:textColor="#FFFFFF"
                        android:textSize="13sp" />
                </LinearLayout>


            </LinearLayout>


        </net.cachapa.expandablelayout.ExpandableLayout>

    </LinearLayout>


</com.app.roadz.modules.MySubscriptions.MySubscriptionsListRow_>